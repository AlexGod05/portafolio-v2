{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@cv": ["./src/utils/cv"],
      "@components/*": ["./src/components/*"],
      "@icons/*": ["./src/icons/*"],
      "@scripts/*": ["./src/scripts/*"],
      "@utils/*": ["./src/utils/*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true
  }
}