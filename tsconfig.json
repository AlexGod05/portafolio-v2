{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@sections/*": ["src/components/sections/*"],
      "@utils/*": ["src/utils/*"],
      "@/*": ["src/*"],
      "@scripts/*": ["src/scripts/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}